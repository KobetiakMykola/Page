{
  "name": "cwp",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "prod": "ts-node -C ttypescript ./src/app.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "ttsc",
    "dev": "ts-node-dev -C ttypescript ./src/app.ts",
    "watch": "nodemon -e ts -w ./src -x npm run watch:serve",
    "watch:serve": "ts-node src/app.ts",
    "create:db": "node_modules/.bin/sequelize db:create",
    "drop:db": "node_modules/.bin/sequelize db:drop",
    "generate:migration": "node_modules/.bin/sequelize migration:generate  --name",
    "migrate:up": "node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo",
    "migrate:undo:all": "node_modules/.bin/sequelize db:migrate:undo:all",
    "generate:seed": "node_modules/.bin/sequelize seed:generate  --name",
    "seed": "node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "node_modules/.bin/sequelize db:seed:undo",
    "seed:undo:all": "node_modules/.bin/sequelize db:seed:undo:all"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "depcheck": "^0.9.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "helmet": "^3.22.0",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mysql": "^2.17.1",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^1.1.0",
    "ts-node": "^8.6.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-validator": "^3.0.0",
    "@types/helmet": "0.0.45",
    "@types/http-errors": "^1.6.3",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/method-override": "0.0.31",
    "@types/morgan": "^1.9.0",
    "@types/mysql": "^2.15.8",
    "@types/passport": "^1.0.2",
    "@types/passport-local": "^1.0.33",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.2",
    "typescript": "^3.7.4"
  },
  "repository": {
    "type": "git",
    "url": ""
  }
}
